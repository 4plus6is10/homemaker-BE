<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
        

<mapper namespace="com.plus6is10.homemaker.repository.BasketRepository">
	<insert id="insertBasket" useGeneratedKyes="true" keyProperty="seq">
        INSERT INTO basket VALUES (#{asin}, #{name}, #{category}, #{price}, #{amount})
    </insert>
</mapper>